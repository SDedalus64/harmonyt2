{
  "name": "harmony",
  "version": "1.0.0",
  "description": "Harmony React Native App",
  "main": "index.ts",
  "scripts": {
    "start": "react-native start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build": "react-native bundle --entry-file index.ts --platform ios --dev false --bundle-output build/main.jsbundle --assets-dest build",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^7.3.17",
    "@react-navigation/drawer": "^7.4.4",
    "@react-navigation/native": "^7.1.13",
    "@react-navigation/native-stack": "^7.3.18",
    "@react-navigation/stack": "^7.3.6",
    "cd": "^0.3.3",
    "expo": "^52.0.6",
    "expo-blur": "~14.0.3",
    "expo-build-properties": "^0.14.0",
    "expo-linear-gradient": "~14.0.2",
    "expo-modules-core": "~2.2.3",
    "expo-screen-orientation": "^8.1.7",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "ios": "^0.0.1",
    "react": "^18.2.0",
    "react-native": "~0.76.6",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "~4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-webview": "~13.12.5"
  },
  "devDependencies": {
    "@react-native-community/cli": "^18.0.0",
    "@react-native/metro-config": "^0.80.0",
    "@types/expo": "33.0.3",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.2.41",
    "@types/react-native": "0.73.0",
    "@types/react-navigation": "^3.4.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^15.2.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@4.9.2"
}
