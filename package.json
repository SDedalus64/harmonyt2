{
  "name": "harmony",
  "version": "1.0.0",
  "description": "Harmony React Native App",
  "main": "index.ts",
  "scripts": {
    "start": "react-native start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build": "react-native bundle --entry-file index.ts --platform ios --dev false --bundle-output build/main.jsbundle --assets-dest build",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.17",
    "@react-navigation/drawer": "^7.4.4",
    "@react-navigation/native": "^7.1.13",
    "@react-navigation/native-stack": "^7.3.18",
    "@react-navigation/stack": "^7.3.6",
    "cd": "^0.3.3",
    "expo-blur": "~14.1.5",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-modules-core": "~2.4.2",
    "expo-screen-orientation": "~8.1.7",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "ios": "^0.0.1",
    "node-fetch": "^2.7.0",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@react-native-community/cli": "^18.0.0",
    "@react-native/eslint-config": "^0.80.0",
    "@react-native/metro-config": "^0.80.0",
    "@types/expo": "33.0.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "@types/react-native": "0.73.0",
    "@types/react-navigation": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "expo": "^53.0.18",
    "husky": "^9.0.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "^30.0.2",
    "lint-staged": "^15.2.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@4.9.2"
}
